{
  "$schema": "../../node_modules/@angular/service-worker/config/schema.json",
  "index": "/kitchen/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/kitchen/favicon.ico",
          "/kitchen/index.html",
          "/kitchen/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/kitchen/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": [
        "/api/kitchen/orders",
        "/api/kitchen/inventory",
        "/api/kitchen/config",
        "/api/kitchen/stats"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "5m",
        "timeout": "10s"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "/api/kitchen/timers/**",
        "/api/kitchen/notifications/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 50,
        "maxAge": "1h"
      }
    }
  ],
  "navigationUrls": [
    "/kitchen/**",
    "!/kitchen/**/*.*",
    "!/kitchen/**/*__*",
    "!/kitchen/**/*__*/**"
  ]
}