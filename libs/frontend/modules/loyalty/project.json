{
  "name": "frontend-modules-loyalty",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/modules/loyalty/src",
  "prefix": "tt",
  "projectType": "library",
  "tags": ["scope:frontend", "type:feature"],
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/frontend/modules/loyalty/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/frontend/modules/loyalty/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/frontend/modules/loyalty/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/frontend/modules/loyalty/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/frontend/modules/loyalty/**/*.ts",
          "libs/frontend/modules/loyalty/**/*.html"
        ]
      }
    }
  }
}